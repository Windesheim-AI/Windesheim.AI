        "eslint-plugin-prettier": "^5.0.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-native": "^4.1.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "expo-updates": "~0.18.17",
