{
    "name": "wingai",
    "version": "1.0.0",
    "main": "node_modules/expo/AppEntry.js",
    "engines": {
        "node": "=16"
    },
    "scripts": {
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "eject": "expo eject",
        "cypress": "cypress open",
        "lint": "echo \"Running eslint check\" && eslint . --max-warnings=0",
        "test:eslint": "npm run lint",
        "lint-fix": "echo \"Running eslint check\" && eslint . --fix --max-warnings=0",
        "test:eslint-fix": "npm run lint-fix",
        "prettier": "echo \"Running prettier check\" && prettier . --check",
        "test:prettier": "npm run prettier",
        "prettier-fix": "echo \"Running prettier check\" && prettier . --write",
        "test:prettier-fix": "npm run prettier-fix",
        "tsc": "echo \"Running typescript check\" && tsc --project tsconfig.lint.json && tsc --project ./cypress/tsconfig.json",
        "test:tsc": "npm run tsc",
        "test:e2e": "concurrently \"npm run web\" \"cypress run --browser electron\"",
        "test:jest": "echo \"Running unit & component tests\" && jest -u --verbose",
        "test:jest-summary": "echo \"Running unit & component tests\" && jest -u",
        "test:jest-less-strict": "echo \"Running unit & component tests\" && jest -u --verbose --detectOpenHandles",
        "test:jest-strict": "echo \"Running unit & component tests\" && jest -u --verbose --detectLeaks --detectOpenHandles",
        "tests": "npm run test:eslint && npm run test:prettier && npm run test:tsc && npm run test:jest",
        "coverage:jest": "echo \"Running unit & component tests\" && jest --coverage --coverageDirectory='coverage-jest'",
        "coverage:e2e": "nyc report --reporter=text-summary",
        "coverage:tests": "npm run coverage:jest && npm run coverage:e2e",
        "coverage:print-env": "npx print-env GITHUB",
        "coverage:set-gh-status": "npx set-gh-status --check-against-readme",
        "coverage:update-badge": "npx update-badge"
    },
    "dependencies": {
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@expo-google-fonts/inter": "^0.2.3",
        "@expo/webpack-config": "^19.0.0",
        "@lottiefiles/react-lottie-player": "^3.5.3",
        "@mui/material": "^5.14.17",
        "@nandorojo/swr-react-native": "^1.2.0",
        "@react-native-async-storage/async-storage": "1.18.2",
        "@react-native-community/eslint-config": "^3.2.0",
        "@react-native-community/netinfo": "9.3.10",
        "@react-native/metro-config": "^0.73.0",
        "@react-navigation/native": "^6.1.9",
        "@react-navigation/native-stack": "^6.9.15",
        "@react-navigation/stack": "^6.3.20",
        "@reduxjs/toolkit": "^1.9.7",
        "@types/react-numeric-input": "^2.2.6",
        "expo": "~49.0.11",
        "expo-font": "~11.4.0",
        "expo-linking": "~5.0.2",
        "expo-router": "^2.0.11",
        "expo-status-bar": "~1.6.0",
        "lottie-react-native": "5.1.6",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-error-boundary": "^4.0.11",
        "react-native": "0.72.6",
        "react-native-animatable": "^1.4.0",
        "react-native-dotenv": "^3.4.9",
        "react-native-error-boundary": "^1.2.3",
        "react-native-fs": "^2.20.0",
        "react-native-linear-gradient": "^2.8.3",
        "react-native-reanimated": "~3.3.0",
        "react-native-render-html": "^6.3.4",
        "react-native-safe-area-context": "4.6.3",
        "react-native-screens": "~3.22.0",
        "react-native-select-dropdown": "^3.4.0",
        "react-native-svg": "14.0.0",
        "react-native-svg-transformer": "^1.1.0",
        "react-native-vector-icons": "^10.0.0",
        "react-native-web": "~0.19.6",
        "react-native-webview": "13.2.2",
        "react-navigation": "^5.0.0",
        "react-numeric-input": "^2.2.3",
        "react-redux": "^8.1.3",
        "redux-mock-store": "^1.5.4",
        "redux-persist": "^6.0.0",
        "swr": "^2.2.4"
    },
    "devDependencies": {
        "@babel/core": "^7.23.2",
        "@bahmutov/print-env": "^2.1.2",
        "@cypress/code-coverage": "^3.12.6",
        "@testing-library/jest-native": "^5.4.3",
        "@testing-library/react-native": "^12.3.0",
        "@types/css-modules": "^1.0.4",
        "@types/jest": "^29.5.5",
        "@types/node": "^20.8.9",
        "@types/react": "~18.2.31",
        "@types/react-dom": "~18.0.10",
        "@types/react-native-vector-icons": "^6.4.15",
        "@types/react-test-renderer": "^18.0.3",
        "@types/redux-mock-store": "^1.0.4",
        "babel-plugin-istanbul": "^6.1.1",
        "check-code-coverage": "^1.10.5",
        "concurrently": "^8.2.2",
        "cypress": "^13.3.0",
        "cypress-cucumber-preprocessor": "^4.3.1",
        "eslint": "^8.52.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-config-universe": "^12.0.0",
        "eslint-plugin-cyclomatic-complexity": "^2.0.11",
        "eslint-plugin-deprecation": "^2.0.0",
        "eslint-plugin-etc": "^2.0.3",
        "eslint-plugin-import": "^2.28.0",
        "eslint-plugin-jest": "^27.4.2",
        "eslint-plugin-prettier": "^5.0.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-native": "^4.1.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "expo-updates": "~0.18.17",
        "jest": "^29.7.0",
        "nyc": "^15.1.0",
        "prettier": "^3.0.3",
        "purgecss": "^5.0.0",
        "react-test-renderer": "18.2.0",
        "typescript": "^5.1.3"
    },
    "private": true
}
